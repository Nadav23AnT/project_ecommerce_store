{
  "name": "ecommerce_store",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "huskyInit": "cd.. && npx husky install && cd .husky/_ && del /f .gitignore",
